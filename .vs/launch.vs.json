{
  "version": "0.2.1",
  "defaults": {},
  "configurations": [
    {
      "type": "cppdbg",
      "name": "D:\\program\\C++\\fightLord",
      "project": "",
      "cwd": "${workspaceRoot}",
      "program": "${workspaceRoot}\\BUILD\\${workspaceRootFolderName}.elf",
      "MIMode": "gdb",
      "externalConsole": true,
      "inheritEnvironments": [
        "gcc_arm"
      ],
      "miDebuggerPath": "${env.gccpath}\\bin\\arm-none-eabi-gdb.exe",
      "setupCommands": [
        {
          "text": "Documentation: Learn how to configure embedded debugging",
          "description": "See here for more info http://aka.ms/vsembeddeddebug",
          "ignoreFailures": true
        },
        {
          "text": "-environment-cd ${workspaceRoot}/BUILD"
        },
        {
          "text": "-file-exec-and-symbols ${workspaceRootFolderName}.elf",
          "description": "load file",
          "ignoreFailures": false
        },
        {
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "visualizerFile": "${debugInfo.linuxNatvisPath}",
      "showDisplayString": true,
      "miDebuggerServerAddress": "localhost:3333",
      "launchCompleteCommand": "None",
      "debugServerPath": "path-to-your\\gdbserver.exe",
      "debugServerArgs": "-f path-to-your/board.cfg",
      "serverStarted": "GDB\\ server\\ started",
      "filterStderr": true,
      "filterStdout": true
    },
    {
      "cwd": "${workspaceRoot}",
      "debugServerArgs": "-f path-to-your/board.cfg",
      "debugServerPath": "path-to-your\\gdbserver.exe",
      "externalConsole": true,
      "filterStderr": true,
      "filterStdout": true,
      "inheritEnvironments": [
        "gcc_arm"
      ],
      "launchCompleteCommand": "None",
      "miDebuggerPath": "${env.gccpath}\\bin\\arm-none-eabi-gdb.exe",
      "miDebuggerServerAddress": "localhost:3333",
      "MIMode": "gdb",
      "name": "D:\\program\\C++\\fightLord(1)",
      "program": "${workspaceRoot}\\BUILD\\${workspaceRootFolderName}.elf",
      "project": "",
      "serverStarted": "GDB\\ server\\ started",
      "setupCommands": [
        {
          "text": "Documentation: Learn how to configure embedded debugging",
          "description": "See here for more info http://aka.ms/vsembeddeddebug",
          "ignoreFailures": true
        },
        {
          "text": "-environment-cd ${workspaceRoot}/BUILD"
        },
        {
          "text": "-file-exec-and-symbols ${workspaceRootFolderName}.elf",
          "description": "load file",
          "ignoreFailures": false
        },
        {
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "showDisplayString": true,
      "type": "cppdbg",
      "visualizerFile": "${debugInfo.linuxNatvisPath}"
    }
  ]
}